{
  "name": "browserslist",
  "version": "0.0.0",
  "author": "Pig Fang <g-plane@hotmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "sync": "node ./scripts/sync.mjs",
    "build:wasm:cjs": "wasm-pack build --target=nodejs --out-dir=pkg/node && node ./scripts/post-node-build-wasm.mjs",
    "build:wasm:cjs:dev": "wasm-pack build --target=nodejs --out-dir=pkg/node --dev && node ./scripts/post-node-build-wasm.mjs",
    "build:wasm:esm": "wasm-pack build --target=web --out-dir=pkg/web && node ./scripts/post-web-build-wasm.mjs",
    "build:wasm:esm:dev": "wasm-pack build --target=web --out-dir=pkg/web --dev && node ./scripts/post-web-build-wasm.mjs"
  },
  "devDependencies": {
    "browserslist": "^4.18.0",
    "electron-to-chromium": "^1.3.896",
    "node-releases": "^2.0.1"
  }
}
